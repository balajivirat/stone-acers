<script>

	$(function($) {

	// settings
	var $slider = $('.slider'); // class or id of carousel slider
	var $slide = 'li'; // could also use 'img' if you're not using a ul
	var $transition_time = 1000; // 1 second
	var $time_between_slides = 4000; // 4 seconds

	function slides(){
	return $slider.find($slide);
	}

	slides().fadeOut();

	// set active classes
	slides().first().addClass('active');
	slides().first().fadeIn($transition_time);

	// auto scroll 
	$interval = setInterval(
	function(){
	var $i = $slider.find($slide + '.active').index();

	slides().eq($i).removeClass('active');
	slides().eq($i).fadeOut($transition_time);

	if (slides().length == $i + 1) $i = -1; // loop to start

	slides().eq($i + 1).fadeIn($transition_time);
	slides().eq($i + 1).addClass('active');
	}
	, $transition_time +  $time_between_slides 
	);

});
//$.noConflict();	
$(document).ready(function() 
{
		if(localStorage.getItem("menulink") == "projects")
		{
//			alert("project click");
			$('html, body').animate({
			scrollTop: $('#projects').offset().top - 125
			}, 'slow');
			localStorage.removeItem("menulink");
		}
	
	getslider();	
	getoffers();
	
	if (localStorage.getItem("projectlist") !== null) 
	{		
		projectlist = JSON.parse(localStorage.getItem("projectlist"));
		console.log('getproject',projectlist);
		$("#content1").empty();
			$.each(projectlist, function(i, item) 
			{
				var dataid = i+1;
				if(projectlist[i].cover_image == null)
				{
					var coverimage = "assets/img/default_image.jpg"
				}
				else
				{
					var coverimage = "https://api.stoneandacres.com"+projectlist[i].cover_image.url;
				}		

				var projecthtml = '<div class="lqd-column col-md-4 col-sm-6 col-xs-12 fade-in" style="cursor:pointer;" onclick="getid('+projectlist[i].id+')"><div class="ld-pf-image"><figure data-responsive-bg="true"><img src="'+coverimage+'" alt="Portfolio"></figure></div><h3 class="ld-pf-title h4 font-weight-bold text-center pb-5">'+projectlist[i].projectname+'</h3></div>';
				setTimeout(function(){
				$("#content1").append(projecthtml);
				}, 100 * i );	
				
			});
	}
	else
	{
		getprojects();
	}
	
	
});
	
	function getslider()
{
			$.ajax({
			type: "GET",
			url: "https://api.stoneandacres.com/homesliders",
			success: function(result) {	
			if(result[0].desktop_slider.length > 0)
			{				
						if (window.matchMedia("(max-width: 767px)").matches)  
						{ 							           
							console.log("This is a mobile device."); 
							var mobile_slider = result[0].mobile_slider;
							console.log("Mobile Slider", mobile_slider);
							$(mobile_slider).each(function(index) {
							var mediaimage = "https://api.stoneandacres.com"+mobile_slider[index].image_url.url;	
							var mediahtml = '<li><img src="'+mediaimage+'" /></li>';
							$("#homepage_slider").append(mediahtml);
							});
						} 
						else 
						{ 							
							console.log("This is a tablet or desktop."); 
							var desktop_slider = result[0].desktop_slider;
							console.log("Desktop Slider", desktop_slider);
							$(desktop_slider).each(function(index) {
							var mediaimage = "https://api.stoneandacres.com"+desktop_slider[index].image_url.url;	
							var mediahtml = '<li><img src="'+mediaimage+'" /></li>';
							$("#homepage_slider").append(mediahtml);
							});
						}				

			}
			else
			{
					console.log("no data");
					$("#homepage_slider").css("display","none");
			}			
			},
			error: function(jqXHR, exception) {
			console.log(jqXHR);
			console.log(exception);
			}
			});
 }
	
function getprojects()
{
	$.ajax({
        type: "GET",
		url: "https://api.stoneandacres.com/projects",
        success: function(result) {	
			localStorage.setItem("projectlist", JSON.stringify(result));			
			console.log('projectlist', JSON.parse(localStorage.getItem("projectlist")));
//		  console.log(result);
			$("#content1").empty();
			$.each(result, function(i, item) 
			{
				var dataid = i+1;
				if(result[i].cover_image == null)
				{
					var coverimage = "assets/img/default_image.jpg"
				}
				else
				{
					var coverimage = "https://api.stoneandacres.com"+result[i].cover_image.url;
				}		

				var projecthtml = '<div class="lqd-column col-md-4 col-sm-6 col-xs-12 fade-in" style="cursor:pointer;" onclick="getid('+result[i].id+')"><div class="ld-pf-image"><figure data-responsive-bg="true"><img src="'+coverimage+'" alt="Portfolio"></figure></div><h3 class="ld-pf-title h4 font-weight-bold text-center pb-5">'+result[i].projectname+'</h3></div>';
				setTimeout(function(){
				$("#content1").append(projecthtml);
				}, 1000 * i );	
				
			});
         },
         error: function(jqXHR, exception) {
         console.log(jqXHR);
         console.log(exception);
         }
         });
 }
	
	function getoffers()
{
	$.ajax({
        type: "GET",
		url: "https://api.stoneandacres.com/offers",
        success: function(result) {	
//		  console.log(result);
			$("#latest_offers").empty();
