			{
				var dataid = i+1;
				if(result[i].offer_image == null)
				{
					var offerimage = "assets/img/default_image.jpg"
				}
				else
				{
					var offerimage = "https://api.stoneandacres.com"+result[i].offer_image.url;
				}
//				console.log(offerimage);		

				var offerhtml = '<div class="lqd-column col-md-4 col-sm-6 col-xs-12"><div class="ld-pf-image"><figure data-responsive-bg="true"><img src="'+offerimage+'" alt="Portfolio"></figure></div></div>	';
				setTimeout(function(){
				$("#latest_offers").append(offerhtml);
				}, 1000 * i );	
				
			});
         },
         error: function(jqXHR, exception) {
         console.log(jqXHR);
         console.log(exception);
         }
         });
 }
	function getid(projectid)
	{

		localStorage.setItem("projectid", projectid);
		window.location.href = "project.html";
		localStorage.setItem("urlpage", "projects");
	}
